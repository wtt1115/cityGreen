/* pages/contactUs/contactUs.wxss */
.contactBox{
  width: 100%;
  height: 100%;
}
.liClass{
  display: flex;
  padding: 40rpx 0;
  margin-left: 30rpx;
  margin-right: 30rpx;
  border-bottom: 1rpx solid #f1f1f1;
  position: relative;
}
.liClass .leftImg{
  width: 36rpx;
  height: 31rpx;
  margin-right: 10rpx;
}
.liClass .rightImg{
  width: 15rpx;
  height: 25rpx;
  position: absolute;
  top:50rpx;
  right:40rpx;
}
.liClass .titleFont{
  font-size: 30rpx;
  color:#333333;
  line-height: 35rpx;
}