<!--pages/feedbackCenter/feedbackCenter.wxml 反馈中心页面-->
<view class="feedbackBox">
  <form bindsubmit="bindFormSubmit">
    <!-- 这一刻想法 -->
    <view class="thinkWay">
      <textarea placeholder="输入信息..." placeholder-class="place" name="textarea" maxlength="-1" value="{{textValue}}"/>
    </view>
    <!-- 立即发布 -->
    <view class="goSubmit">
      <button form-type="submit">提交信息</button>
    </view>
  </form>
</view>
